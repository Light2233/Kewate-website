generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}



model CommercialOffer {
  id              Int     @id @default(autoincrement())
  name            String
  clientName      String
  service         String
  projectScope    String
  developmentTime String
  cost            String
  url             String
  userProblems    Json[]
  projects        String[]
  clientGetting   Json[]
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
}